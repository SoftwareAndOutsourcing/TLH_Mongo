<script type="text/javascript">
// Example starter JavaScript for disabling form submissions if there are invalid fields
// DO NOT REMOVE - TBrannon (08/28/2021) -----------------------------------------------
(function () {
  'use strict'

  // Fetch all the forms we want to apply custom Bootstrap validation styles to
  var forms = document.querySelectorAll('.needs-validation')

  // Loop over them and prevent submission
  Array.prototype.slice.call(forms)
    .forEach(function (form) {
      form.addEventListener('submit', function (event) {
        if (!form.checkValidity()) {
          event.preventDefault()
          event.stopPropagation()
        }

        form.classList.add('was-validated')
      }, false)
    })
})()
</script>

<script>

  // Autofill 'payment due date' field with date 10 days out from 'sale date'  - ADDED 9/25/2021
  $(document).ready(function() {
    $(document).on('change', '.form-control-saledate', function() {
      var saleDate;
      saleDate = $(' #saledate ').val();
      var targetDate = new Date(saleDate);
      targetDate.setDate(targetDate.getDate() + 11);
      var dd = ("0" + targetDate.getDate()).substr(-2);
      var mm = ("0" + (targetDate.getMonth() + 1)).substr(-2);
      var yyyy = targetDate.getFullYear();
      var dateString = [yyyy, mm, dd].join('-');
    
      // trigger() does not affect addEventListener callbacks
      let o = $(' #paymentduedate ');
      o.val(dateString).trigger('change');
      if (o.length)
        o[0].dispatchEvent(new Event('change'));
      confirmElementHasValue(o[0]); // call section-logic.js method
    });
  });
</script>


